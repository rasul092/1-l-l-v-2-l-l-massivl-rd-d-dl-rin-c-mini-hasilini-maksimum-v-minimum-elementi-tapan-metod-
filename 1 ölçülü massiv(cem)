#include <iostream>
using namespace std;

void print() { // 1 ölçülü massivdə ədədlərin cəmi
    int array[]={2,1,5,7,8,4,5};
    int n=sizeof(array)/ sizeof(array[0]);
   
    int sum=0;
    for (int i=0; i<n; i++){
        sum+=array[i];   
    }
    cout << "Massivdəki ədədlərin cəmi : " << sum;
}

int main() {
    print(); // funksiyanı çağırırıq
    return 0;
}
