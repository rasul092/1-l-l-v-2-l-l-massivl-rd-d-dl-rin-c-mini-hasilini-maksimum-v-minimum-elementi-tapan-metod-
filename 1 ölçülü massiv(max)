#include <iostream>
using namespace std;


int main() { // 1 olculu matrisde ededlerin en boyuyunu tapan kod
   int array[]={7,2,1,5,21,52};
   int n=sizeof(array)/ sizeof(array[0]);
   int max=array[0];
   for (int i=0; i<n;i++){
    if(array[i]>max) max=array[i];   
   }
   cout<<"Massivdeki ededlerin en boyuyu : "<<max;
  
   
   

    return 0;
}
