#include <iostream>
using namespace std;

void print() { // 2 ölçülü massivdə ədədlərin cəmini tapan funksiya
    int matrix[3][3] = {
        {2,5,6},
        {1,4,3},
        {7,1,6}
    };

    int sum = 0;
    int n = sizeof(matrix) / sizeof(matrix[0]);

    for(int i=0; i<n; i++){
        for(int j=0; j<n; j++){
            sum += matrix[i][j];
        }
    }
    cout << "Massivdəki ədədlərin cəmi : " << sum << endl;
}

int main() {
    print(); 
    return 0;
}

