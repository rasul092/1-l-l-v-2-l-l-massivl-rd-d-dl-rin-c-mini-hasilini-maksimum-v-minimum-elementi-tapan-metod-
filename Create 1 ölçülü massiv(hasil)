#include <iostream>
using namespace std;


int main() { // 1 olculu matrisde ededlerin hasili
   int array[]={2,1,5,7,8,4,5};
   int n=sizeof(array)/ sizeof(array[0]);
   
   int product=1;
   for (int i=0; i<n;i++){
    product*=array[i];   
   }
   cout<<"Massivdeki ededlerin hasili : "<<product;
  
   
   

    return 0;
}
